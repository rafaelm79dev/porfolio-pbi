	measure Current_Pressure = FORMAT(sum(current_df[surface_pressure]), "####") & " hPa"
		lineageTag: 35827656-5043-4c1a-8884-d9099f2d6843

	measure Current_Pressure_Desc =
			
			VAR pressure = sum(current_df[surface_pressure])
			VAR selectedLang = SELECTCOLUMNS (
			        SUMMARIZE (
			            languageParam,
			            languageParam[language],
			            languageParam[Parameter Fields]
			        ),
			        languageParam[language]
			    )
			VAR Category =
			    SWITCH (
			        TRUE(),
			        pressure > 1013, "High",
			        pressure >= 1010 && pressure <= 1013, "Medium",
			        pressure < 1010, "Low"
			    )
			RETURN
			    SWITCH (
			        selectedLang,
			        "Español",
			            SWITCH (
			                Category,
			                "High", "Alta",
			                "Medium", "Media",
			                "Low", "Baja"
			            ),
			        Category   // por defecto devuelve la categoría en inglés
			    )
		lineageTag: e7d89026-e861-46e8-9e9b-89fc998cf46b

