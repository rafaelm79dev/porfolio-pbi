	measure Current_Title_WindDirection =
			
			VAR Deg = MOD ([Current_WindDirection], 360 )
			var selectedLang = SELECTCOLUMNS (
			        SUMMARIZE (
			            languageParam,
			            languageParam[language],
			            languageParam[Parameter Fields]
			        ),
			        languageParam[language]
			    )
			VAR Direction =
			    SWITCH (
			        TRUE(),
			        Deg >= 337.5 || Deg < 22.5, "North",
			        Deg >= 22.5 && Deg < 67.5, "Northeast",
			        Deg >= 67.5 && Deg < 112.5, "East",
			        Deg >= 112.5 && Deg < 157.5, "Southeast",
			        Deg >= 157.5 && Deg < 202.5, "South",
			        Deg >= 202.5 && Deg < 247.5, "Southwest",
			        Deg >= 247.5 && Deg < 292.5, "West",
			        Deg >= 292.5 && Deg < 337.5, "Northwest"
			    )
			RETURN SWITCH (
			        selectedLang,
			        "Español",
			            SWITCH (
			                Direction,
			                "North", "Norte",
			                "Northeast", "Noreste",
			                "East", "Este",
			                "Southeast", "Sudeste",
			                "South", "Sur",
			                "Southwest", "Sudoeste",
			                "West", "Oeste",
			                "Northwest", "Noroeste"
			            ),
			        Direction  // por defecto devuelve en inglés
			    )
		lineageTag: 287617f4-347b-43a4-8f01-974cf8f54f88

